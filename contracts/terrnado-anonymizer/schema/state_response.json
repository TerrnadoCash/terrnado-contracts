{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "StateResponse",
  "type": "object",
  "required": [
    "deposits_number",
    "unique_users",
    "withdrawals_number"
  ],
  "properties": {
    "deposits_number": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "unique_users": {
      "type": "integer",
      "format": "uint",
      "minimum": 0.0
    },
    "withdrawals_number": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    }
  }
}
